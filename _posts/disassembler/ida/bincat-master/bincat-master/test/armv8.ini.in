#here addresses are supposed to be absolute and not of the form ds:o
mem[0x1000*8192] = |00|?0xFF


[libc]
#tainting rules for libc
#default calling convention for this lib
# call_conv = fastcall
* = open(@, _)
#read uses a different calling convention
* = read<stdcall>(@, *, @)


